<% experience.each do |exp| %>
  <div class="experience_item">
      <strong><%= exp.title %></strong> <%= link_to 'Edit', edit_experience_path(exp), {:remote => true, 'data-toggle' => "modal", 'data-target' => '#EditExperience'} if current_user %>
      <br>
      <p><%= exp.organization %></p>
      <p><%= exp.location %></p>
      <p>Started: <%= exp.date_started %> Ended: <%= exp.date_ended %></p>
      <p><%= exp.body %></p>
      <br>
  </div>
<% end %>
<%= link_to 'Add new', new_experience_path, {:remote => true, 'data-toggle' => "modal", 'data-target' => '#createNewExperience'} if current_user %>
